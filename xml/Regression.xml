<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Regression Suite">
	<groups>
		<run>
			<include name="Regression"></include>
		</run>
	</groups>
	<listeners>
		<listener
			class-name="com.facebook.listeners.FacebookListener"></listener>
	</listeners> 
  <test  name="Test">
    <classes>
      <class name="com.facebook.registrationTests.RegistrationTest2"/>
      <class name="com.facebook.registrationTests.RegistrationTest"/>
      <class name="com.facebook.loginTests.LoginTest"/>
      <class name="com.facebook.loginTests.LoginFunctionalTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Regression Suite -->
